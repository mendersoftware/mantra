{
  "extends": "@northern.tech/typescript-config/react-app.json",
  "compilerOptions": {
    "incremental": true,
    "module": "esnext",
    "paths": {
      "@/*": ["./*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strict": false,
    "target": "ES2020"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"]
}
